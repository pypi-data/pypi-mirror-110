#!/Users/MGula/Documents/Python/mlb_showdown_card_bot/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mlb-showdown-bot==2.4.0','console_scripts','showdownbot'
__requires__ = 'mlb-showdown-bot==2.4.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mlb-showdown-bot==2.4.0', 'console_scripts', 'showdownbot')()
    )
