CREATE TABLE `watchmen`.`topics` (
  `topicId` VARCHAR(60) NOT NULL,
  `name` VARCHAR(45) NULL,
  `kind` VARCHAR(45) NULL,
  `type` VARCHAR(45) NULL,
  `description` VARCHAR(45) NULL,
  `version` INT NULL,
  `factors` JSON NULL,
  `createTime` VARCHAR(45) NULL,
  `last_modified` DATETIME NULL,
  PRIMARY KEY (`topicId`));